{
  "name": "quiz",
  "version": "1.0.0",
  "description": "General Knowledge Quiz MCP - Interactive widget with 8 questions across various topics",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run build:widgets && wrangler deploy",
    "type-check": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "build:widget:quiz": "INPUT=widgets/quiz.html vite build",
    "build:widgets": "npm run build:widget:quiz",
    "dev:widget": "INPUT=widgets/quiz.html vite build --watch",
    "dev:serve": "cd web && vite preview",
    "dev:all": "concurrently \"npm run dev:widget\" \"npm run dev:serve\"",
    "serve": "cd web && vite preview",
    "start": "npm run dev:all"
  },
  "dependencies": {
    "@cloudflare/workers-oauth-provider": "^0.1.0",
    "@modelcontextprotocol/ext-apps": "^0.2.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@workos-inc/node": "^7.73.0",
    "agents": "^0.2.14",
    "body-parser": ">=2.2.1",
    "hono": "^4.10.3",
    "jose": "^6.1.0",
    "pilpat-mcp-security": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.9.2",
    "vite": "^6.0.5",
    "vite-plugin-singlefile": "^2.0.2",
    "wrangler": "^4.40.1"
  }
}
